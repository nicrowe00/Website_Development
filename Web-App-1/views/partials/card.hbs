<div class="ui card" id ="{{name}}">
  <div class="image">
    <img src="{{image}}">
  </div>
  <div class="content">
    <a class="header">{{fullname}}</a>
    <div class="meta">
      <span class="date">Born in {{birth}}</span>
    </div>
    <div class="description">
      {{name}} is known for {{gender}} role in {{role}} as {{rolename}}
    </div>
  </div>
</div>